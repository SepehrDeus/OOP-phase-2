<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="fxml.post.MyPostsController">
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
   </padding>
   <Label alignment="CENTER" layoutX="251.0" layoutY="36.0" prefHeight="105.0" prefWidth="398.0" text="MY POSTS">
      <font>
         <Font size="62.0"/>
      </font>
   </Label>
   <Button fx:id="CreatePostButton" layoutX="73.0" layoutY="194.0" mnemonicParsing="false" onAction="#go_to_create_post"
           prefHeight="113.0" prefWidth="224.0" text="CREATE POST"/>
   <Button fx:id="ShowCommentsButton" layoutX="615.0" layoutY="367.0" mnemonicParsing="false"
           onAction="#go_to_show_comments" prefHeight="113.0" prefWidth="224.0" text="SHOW COMMENTS"/>
   <Button fx:id="ShowLikesButton" layoutX="338.0" layoutY="367.0" mnemonicParsing="false" onAction="#go_to_show_likes"
           prefHeight="113.0" prefWidth="224.0" text="SHOW LIKES"/>
   <Button fx:id="ShowStatsButton" layoutX="73.0" layoutY="367.0" mnemonicParsing="false" onAction="#go_to_show_stats"
           prefHeight="113.0" prefWidth="224.0" text="SHOW STATS"/>
   <Button fx:id="DeletePostButton" layoutX="338.0" layoutY="194.0" mnemonicParsing="false"
           onAction="#go_to_delete_post" prefHeight="113.0" prefWidth="224.0" text="DELETE POST"/>
   <Button fx:id="UpdatePostButton" layoutX="615.0" layoutY="194.0" mnemonicParsing="false"
           onAction="#go_to_update_caption" prefHeight="113.0" prefWidth="224.0" text="UPDATE POST"/>
   <Button fx:id="ReturnHomePageButton" layoutX="73.0" layoutY="523.0" mnemonicParsing="false"
           onAction="#Return_HomePage" prefHeight="47.0" prefWidth="88.0" text="RETURN"/>
</AnchorPane>
